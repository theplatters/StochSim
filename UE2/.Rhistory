ex3.sampler <- function(size){
i <- 0
l <- list()
while(i <= size){
x = runif(1,min = 0,max = 1/(exp(1) -1))
y = runif(1)
if(y <= x){
l <- c(l,y)
i = i+1
}
}
return(l)
}
